generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
}

model Banner {
  id            Int      @id @default(autoincrement())
  image         Json
  description   String?
  capture       String?
  createdAt     DateTime @default(now())
}